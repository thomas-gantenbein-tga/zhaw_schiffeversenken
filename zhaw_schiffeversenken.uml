              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_SwOVwP26Eeef4b7VB80lew" name="paintComponent" specification="_SugegP26Eeef4b7VB80lew">
                <ownedParameter xmi:id="_SwOVwf26Eeef4b7VB80lew" name="g" type="_Ss58Af26Eeef4b7VB80lew"/>
                <language>java</language>
                <body>super.paintComponent(g);&#xD;
		Graphics2D g2 = (Graphics2D) g;&#xD;
		g2.setStroke(new BasicStroke(2));&#xD;
&#xD;
		if (this.getWidth() > this.getHeight()) {&#xD;
			playingFieldPanelSize = this.getHeight();&#xD;
		} else {&#xD;
			playingFieldPanelSize = this.getWidth();&#xD;
		}&#xD;
&#xD;
		for (Shape shape : shapes) {&#xD;
			shape.draw(playingFieldPanelSize, g2);&#xD;
		}&#xD;
	</body>
              </ownedBehavior>
