                <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_SvTIsv26Eeef4b7VB80lew" value="*"/>
                <defaultValue xmi:type="uml:LiteralString" xmi:id="_SvTvwP26Eeef4b7VB80lew" value="new ArrayList&lt;Shape>()"/>
              </ownedAttribute>
              <ownedAttribute xmi:id="_SvUW0P26Eeef4b7VB80lew" name="playingFieldPanelSize" visibility="private">
