              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_SwD9sP26Eeef4b7VB80lew" name="processPlayersShot" specification="_Sskk0f26Eeef4b7VB80lew">
                <ownedParameter xmi:id="_SwD9sf26Eeef4b7VB80lew" name="shotCoordinate" type="_ShJ_AP26Eeef4b7VB80lew"/>
                <language>java</language>
                <body>if (computerField.processShot(shotCoordinate)) {
			if (getSwimmingShips(computerField) != 0) {
				playerField.processShot(computerPlayer.makeLogicShot(playerField));
			}
			alertDisplays();
		}
	</body>
              </ownedBehavior>
