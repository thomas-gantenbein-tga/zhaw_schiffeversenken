              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_SwKrYP26Eeef4b7VB80lew" name="Ship" specification="_SfWoMP26Eeef4b7VB80lew">
                <ownedParameter xmi:id="_SwKrYf26Eeef4b7VB80lew" name="colCount">
                  <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
                </ownedParameter>
                <ownedParameter xmi:id="_SwKrYv26Eeef4b7VB80lew" name="rowCount">
