              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_SwFL0P26Eeef4b7VB80lew" name="alertDisplays" specification="_Se58QP26Eeef4b7VB80lew">
                <language>java</language>
                <body>for (Display display : displayList) {
