                <body>First screen shown to users. Let's them select the size of the playing fields
of human and computer player.

 *</body>
              </ownedComment>
              <ownedAttribute xmi:id="_SvadcP26Eeef4b7VB80lew" name="frame" visibility="private" type="_SthnEf26Eeef4b7VB80lew" association="_SxCOEf26Eeef4b7VB80lew"/>
              <ownedAttribute xmi:id="_SvbEgf26Eeef4b7VB80lew" name="sizeComputerFieldInput" visibility="private" type="_Sti1MP26Eeef4b7VB80lew" association="_SxC1If26Eeef4b7VB80lew"/>
              <ownedAttribute xmi:id="_Svbrkf26Eeef4b7VB80lew" name="sizePlayerFieldInput" visibility="private" type="_Sti1MP26Eeef4b7VB80lew" association="_SxDcMf26Eeef4b7VB80lew"/>
              <ownedAttribute xmi:id="_SvcSof26Eeef4b7VB80lew" name="sizeComputerField" visibility="private">
                <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
              </ownedAttribute>
              <ownedAttribute xmi:id="_Svc5sf26Eeef4b7VB80lew" name="sizePlayerField" visibility="private">
                <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
              </ownedAttribute>
              <ownedAttribute xmi:id="_Svdgwf26Eeef4b7VB80lew" name="MAX_FIELD_SIZE" visibility="private" isLeaf="true" isReadOnly="true">
                <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
                <defaultValue xmi:type="uml:LiteralString" xmi:id="_Svdgwv26Eeef4b7VB80lew" value="30"/>
              </ownedAttribute>
              <ownedAttribute xmi:id="_SveH0f26Eeef4b7VB80lew" name="MIN_FIELD_SIZE" visibility="private" isLeaf="true" isReadOnly="true">
                <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
                <defaultValue xmi:type="uml:LiteralString" xmi:id="_SveH0v26Eeef4b7VB80lew" value="6"/>
