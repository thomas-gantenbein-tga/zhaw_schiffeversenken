              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_Swfbg_26Eeef4b7VB80lew" name="testIsSunk" specification="_ShIJ0_26Eeef4b7VB80lew">
                <language>java</language>
                <body>Ship ship = new Ship(coordinates);
		for(Coordinate coordinate : coordinates) {
			coordinate.setIsHit(true);;
		}
		
		Assert.assertTrue(ship.isSunk());
		coordinates.get(0).setIsHit(false);
		Assert.assertFalse(ship.isSunk());

	</body>
