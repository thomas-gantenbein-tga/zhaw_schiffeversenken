                </ownedComment>
                <generalization xmi:id="_St9r8P26Eeef4b7VB80lew" general="_SgbmQP26Eeef4b7VB80lew"/>
                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_SwbKEP26Eeef4b7VB80lew" name="TailPreviewShape" specification="_Su78UP26Eeef4b7VB80lew">
                  <ownedParameter xmi:id="_SwbKEf26Eeef4b7VB80lew" name="centerX">
