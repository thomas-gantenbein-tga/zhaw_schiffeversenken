              <ownedAttribute xmi:id="_SvLM4P26Eeef4b7VB80lew" name="shipPositions" visibility="package" type="_ShJ_AP26Eeef4b7VB80lew" isOrdered="true" association="_Sw7gYf26Eeef4b7VB80lew">
