                </ownedComment>
                <generalization xmi:id="_St8d0P26Eeef4b7VB80lew" general="_SgbmQP26Eeef4b7VB80lew"/>
                <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_SwajA_26Eeef4b7VB80lew" name="ShipSunk" specification="_Su6HIP26Eeef4b7VB80lew">
                  <ownedParameter xmi:id="_SwajBP26Eeef4b7VB80lew" name="centerX">
                    <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#double"/>
                  </ownedParameter>
