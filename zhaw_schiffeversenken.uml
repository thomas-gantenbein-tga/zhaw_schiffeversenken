              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_Swfbgv26Eeef4b7VB80lew" name="testIsHit" specification="_ShHiw_26Eeef4b7VB80lew">
                <language>java</language>
                <body>Ship ship = new Ship(coordinates);
		Assert.assertFalse(ship.isHit(noShipCoordinate));
