                  </ownedParameter>
                  <ownedParameter xmi:id="_SwW4pf26Eeef4b7VB80lew" name="rowCount">
                    <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
                  </ownedParameter>
                  <ownedParameter xmi:id="_SwW4pv26Eeef4b7VB80lew" name="columnCount">
                    <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
                  </ownedParameter>
                  <ownedParameter xmi:id="_SwW4p_26Eeef4b7VB80lew" name="width">
                    <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
                  </ownedParameter>
                  <ownedParameter xmi:id="_SwW4qP26Eeef4b7VB80lew" name="height">
                    <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
                  </ownedParameter>
                  <ownedParameter xmi:id="_SwW4qf26Eeef4b7VB80lew" type="_SgbmQP26Eeef4b7VB80lew" direction="return"/>
                  <language>java</language>
                  <body>double xPosition;&#xD;
		double yPosition;&#xD;
&#xD;
		if (height == 0) {&#xD;
			xPosition = 50;&#xD;
			yPosition = index * 100.0 / rowCount;&#xD;
		} else {&#xD;
			xPosition = index * 100.0 / columnCount;&#xD;
			yPosition = 50;&#xD;
		}&#xD;
&#xD;
		Shape line = new Line(xPosition, yPosition, width, height, Color.GRAY);&#xD;
		return line;&#xD;
	</body>
                </ownedBehavior>
