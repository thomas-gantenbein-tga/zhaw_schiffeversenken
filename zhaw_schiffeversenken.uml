              <ownedAttribute xmi:id="_SvhLIf26Eeef4b7VB80lew" name="sizeComputerField" visibility="private">
                <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
              </ownedAttribute>
              <ownedAttribute xmi:id="_SvhyMf26Eeef4b7VB80lew" name="sizePlayerField" visibility="private">
