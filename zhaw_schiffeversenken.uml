                <ownedParameter xmi:id="_SwNHp_26Eeef4b7VB80lew" type="_SflRsP26Eeef4b7VB80lew" direction="return"/>
                <language>java</language>
                <body>this.panel = panel;
		this.columnCount = columnCount;
		this.rowCount = rowCount;
	</body>
