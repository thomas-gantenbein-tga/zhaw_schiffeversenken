              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_SwbxJP26Eeef4b7VB80lew" name="makeRandomShot" specification="_SgtTEP26Eeef4b7VB80lew">
                <ownedParameter xmi:id="_SwbxJf26Eeef4b7VB80lew" type="_ShJ_AP26Eeef4b7VB80lew" direction="return"/>
                <language>java</language>
                <body>int xCoordinate = randomGenerator.nextInt(rowCount);
		int yCoordinate = randomGenerator.nextInt(columnCount);
		Coordinate coordinate = new Coordinate(xCoordinate, yCoordinate, null, null);
		return coordinate;
	</body>
