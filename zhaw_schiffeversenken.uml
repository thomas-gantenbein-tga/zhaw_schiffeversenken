              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_SwMgkP26Eeef4b7VB80lew" name="getWoundPositions" specification="_SfbgsP26Eeef4b7VB80lew">
                <ownedParameter xmi:id="_SwMgkf26Eeef4b7VB80lew" type="_ShJ_AP26Eeef4b7VB80lew" isOrdered="true" direction="return">
                  <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_SwMgkv26Eeef4b7VB80lew" value="*"/>
                </ownedParameter>
                <language>java</language>
                <body>ArrayList&lt;Coordinate> shipWoundPositions = new ArrayList&lt;Coordinate>();
		for (Coordinate shipPosition : shipPositions) {
			if (shipPosition.getIsHit())
				shipWoundPositions.add(shipPosition);
		}
		return shipWoundPositions;
	</body>
              </ownedBehavior>
              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_SwMgk_26Eeef4b7VB80lew" name="getDirectionsOfHits" specification="_Sfcu0P26Eeef4b7VB80lew">
                <ownedParameter xmi:id="_SwMglP26Eeef4b7VB80lew" type="_SgvvUP26Eeef4b7VB80lew" direction="return"/>
                <language>java</language>
                <body>List&lt;Coordinate> woundPositions = getWoundPositions();
		if (woundPositions.get(0).getxPosition() == woundPositions.get(1).getxPosition())
			return Directions.NORTH;
		else
			return Directions.WEST;
	</body>
              </ownedBehavior>
